<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="309.0" prefWidth="562.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pterapan.collectiondemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
      <children>
         <VBox prefHeight="269.0" prefWidth="235.0">
            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Tambah Student Baru">
                  <VBox.margin>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </VBox.margin>
               </Text>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="NRP:">
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
               </Text>
               <TextField fx:id="txtNrp">
                  <VBox.margin>
                     <Insets bottom="5.0" right="5.0" />
                  </VBox.margin>
               </TextField>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nama:">
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
               </Text>
               <TextField fx:id="txtNama">
                  <VBox.margin>
                     <Insets bottom="5.0" right="5.0" />
                  </VBox.margin>
               </TextField>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nilai IPA:" />
               <TextField fx:id="txtIpa">
                  <VBox.margin>
                     <Insets bottom="5.0" right="5.0" />
                  </VBox.margin>
               </TextField>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nilai IPS:">
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
               </Text>
               <TextField fx:id="txtIps">
                  <VBox.margin>
                     <Insets bottom="5.0" right="5.0" />
                  </VBox.margin>
               </TextField>
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <children>
                     <Button fx:id="btnTambah" mnemonicParsing="false" onAction="#onTambah" prefHeight="25.0" prefWidth="63.0" text="Tambah">
                        <HBox.margin>
                           <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </HBox.margin>
                     </Button>
                     <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#onUpdate" prefHeight="25.0" prefWidth="63.0" text="Update">
                        <HBox.margin>
                           <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </HBox.margin>
                     </Button>
                     <Button fx:id="btnHapus" mnemonicParsing="false" onAction="#onHapus" prefHeight="25.0" prefWidth="63.0" text="Hapus">
                        <HBox.margin>
                           <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                        </HBox.margin>
                     </Button>
                  </children>
               </HBox>
            </children>
         </VBox>
         <TableView fx:id="table1" onMouseClicked="#getSelectedItem" prefHeight="255.0" prefWidth="334.0">
           <columns>
             <TableColumn fx:id="column1" prefWidth="75.0" text="C1" />
             <TableColumn fx:id="column2" prefWidth="75.0" text="C2" />
               <TableColumn fx:id="column3" prefWidth="75.0" text="C3" />
               <TableColumn fx:id="column4" prefWidth="75.0" text="C4" />
           </columns>
         </TableView>
      </children>
   </HBox>
</VBox>
